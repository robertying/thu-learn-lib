{
  "name": "thu-learn-lib",
  "version": "4.0.0",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "./lib/module/index.js",
  "exports": {
    ".": {
      "types": "./lib/typescript/index.d.ts",
      "default": "./lib/module/index.js"
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      "typescript"
    ]
  },
  "scripts": {
    "build": "bob build",
    "dev": "vite",
    "dev:build": "vite build",
    "format": "prettier --write \"{src,test,demo}/**/*.{ts,html,json}\" \"*.{ts,md,json}\"",
    "lint": "eslint",
    "test": "OPENSSL_CONF=openssl.cnf vitest run"
  },
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "author": "Harry Chen <harry-chen@outlook.com> (https://harrychen.xyz)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/sm-crypto": "^0.3.4",
    "@types/tough-cookie": "^4.0.5",
    "@types/webextension-polyfill": "^0.12.4",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "domhandler": "^5.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "fake-parse5": "^0.0.2",
    "prettier": "^3.7.4",
    "react-native-builder-bob": "^0.40.16",
    "terser": "^5.44.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vite-plugin-web-extension": "^4.5.0",
    "vitest": "^4.0.15",
    "webextension-polyfill": "^0.12.0"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "entities": "^7.0.0",
    "fetch-cookie": "3.0.1",
    "js-base64": "^3.7.8",
    "node-fetch-native": "^1.6.7",
    "sm-crypto": "^0.3.13",
    "tough-cookie": "4.1.4"
  },
  "packageManager": "yarn@4.12.0+sha512.f45ab632439a67f8bc759bf32ead036a1f413287b9042726b7cc4818b7b49e14e9423ba49b18f9e06ea4941c1ad062385b1d8760a8d5091a1a31e5f6219afca8"
}
