{
  "name": "thu-learn-lib",
  "version": "4.0.0",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/commonjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "esm": true
        }
      ]
    ]
  },
  "scripts": {
    "build": "bob build",
    "dev": "vite",
    "dev:build": "vite build",
    "format": "prettier --write \"{src,test,demo}/**/*.{ts,html,json}\" \"*.{ts,md,json}\"",
    "lint": "eslint",
    "test": "OPENSSL_CONF=openssl.cnf vitest run"
  },
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "author": "Harry Chen <harry-chen@outlook.com> (https://harrychen.xyz)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@types/tough-cookie": "^4.0.5",
    "@types/webextension-polyfill": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "domhandler": "^5.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "fake-parse5": "^0.0.1",
    "prettier": "^3.4.2",
    "react-native-builder-bob": "^0.35.2",
    "terser": "^5.37.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.6",
    "vite-plugin-web-extension": "^4.3.1",
    "vitest": "^2.1.8",
    "webextension-polyfill": "^0.12.0"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "entities": "^6.0.0",
    "fetch-cookie": "^3.1.0",
    "js-base64": "^3.7.7",
    "node-fetch-native": "^1.6.4",
    "tough-cookie": "^5.0.0"
  },
  "packageManager": "yarn@4.5.1+sha512.341db9396b6e289fecc30cd7ab3af65060e05ebff4b3b47547b278b9e67b08f485ecd8c79006b405446262142c7a38154445ef7f17c1d5d1de7d90bf9ce7054d"
}
