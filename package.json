{
  "name": "thu-learn-lib",
  "version": "3.2.0",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/commonjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "esm": true
        }
      ]
    ]
  },
  "scripts": {
    "build": "bob build",
    "dev": "vite",
    "dev:build": "vite build",
    "format": "prettier --write \"{src,test,demo}/**/*.{ts,html,json}\" \"*.{ts,md,json}\"",
    "lint": "eslint --ext .ts 'src/**'",
    "test": "OPENSSL_CONF=openssl.cnf vitest run"
  },
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "author": "Harry Chen <harry-chen@outlook.com> (https://harrychen.xyz)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "devDependencies": {
    "@types/node": "22.7.7",
    "@types/tough-cookie": "^4.0.5",
    "@types/webextension-polyfill": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "domhandler": "^5.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "fake-parse5": "^0.0.1",
    "prettier": "^3.3.3",
    "react-native-builder-bob": "^0.30.2",
    "terser": "^5.36.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vite-plugin-web-extension": "^4.2.0",
    "vitest": "^2.1.3",
    "webextension-polyfill": "^0.12.0"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "entities": "^5.0.0",
    "fetch-cookie": "^3.0.1",
    "js-base64": "^3.7.7",
    "node-fetch-native": "^1.6.4",
    "tough-cookie": "^5.0.0"
  },
  "packageManager": "yarn@1.22.22+sha256.c17d3797fb9a9115bf375e31bfd30058cac6bc9c3b8807a3d8cb2094794b51ca"
}
