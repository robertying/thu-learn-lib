{
  "name": "thu-learn-lib",
  "version": "4.0.0",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "./lib/module/index.js",
  "exports": {
    ".": {
      "types": "./lib/typescript/index.d.ts",
      "default": "./lib/module/index.js"
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      "typescript"
    ]
  },
  "scripts": {
    "build": "bob build",
    "dev": "vite",
    "dev:build": "vite build",
    "format": "prettier --write \"{src,test,demo}/**/*.{ts,html,json}\" \"*.{ts,md,json}\"",
    "lint": "eslint",
    "test": "OPENSSL_CONF=openssl.cnf vitest run"
  },
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "author": "Harry Chen <harry-chen@outlook.com> (https://harrychen.xyz)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.3.3",
    "@types/sm-crypto": "^0.3.4",
    "@types/tough-cookie": "^4.0.5",
    "@types/webextension-polyfill": "^0.12.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "domhandler": "^5.0.3",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "fake-parse5": "^0.0.2",
    "prettier": "^3.6.2",
    "react-native-builder-bob": "^0.40.13",
    "terser": "^5.44.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vite-plugin-web-extension": "^4.4.5",
    "vitest": "^3.2.4",
    "webextension-polyfill": "^0.12.0"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "entities": "^7.0.0",
    "fetch-cookie": "3.0.1",
    "js-base64": "^3.7.8",
    "node-fetch-native": "^1.6.7",
    "sm-crypto": "^0.3.13",
    "tough-cookie": "4.1.4"
  },
  "packageManager": "yarn@4.9.4+sha512.7b1cb0b62abba6a537b3a2ce00811a843bea02bcf53138581a6ae5b1bf563f734872bd47de49ce32a9ca9dcaff995aa789577ffb16811da7c603dcf69e73750b"
}
