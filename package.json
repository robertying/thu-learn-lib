{
  "name": "thu-learn-lib",
  "version": "3.1.3",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "type": "module",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "scripts": {
    "build": "bob build",
    "dev": "vite",
    "dev:build": "vite build",
    "format": "prettier --write \"{src,test,demo}/**/*.{ts,html,json}\" \"*.{ts,md,json}\"",
    "lint": "eslint --ext .ts 'src/**'",
    "test": "vitest run"
  },
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "author": "Harry Chen <harry-chen@outlook.com> (https://harrychen.xyz)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "devDependencies": {
    "@types/node": "20.7.0",
    "@types/tough-cookie": "^4.0.3",
    "@types/webextension-polyfill": "^0.10.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "domhandler": "^5.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "fake-parse5": "^0.0.1",
    "prettier": "^3.0.3",
    "react-native-builder-bob": "^0.23.1",
    "terser": "^5.20.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-web-extension": "^3.2.0",
    "vitest": "^0.34.5",
    "webextension-polyfill": "^0.10.0"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "entities": "^4.5.0",
    "fetch-cookie": "^2.1.0",
    "js-base64": "^3.7.5",
    "node-fetch-cookie-native": "^0.0.2",
    "node-fetch-native": "^1.4.0",
    "tough-cookie": "^4.1.3"
  }
}
